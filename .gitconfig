[user]
	name = Chris Callahan
	email = callahan.c@gmail.com
[credential]
    helper = osxkeychain
[core]
	whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
	excludesfile = /Users/cc/.gitignore
[alias]
    st = status
    co = checkout
    rec = add -p
    br = branch
    last = log @{1}..
    up = remote update
	ci = commit
	df = diff
	lg = log -p
	who = shortlog -s --
	pr = pull --rebase
[push]
    default = simple
[color]
	ui = true
	diff = auto
	status = auto
	branch = auto
[format]
	pretty = %C(yellow)%h%Creset %s %C(red)(%an, %cr)%Creset
[url "git@github.com:"]
	insteadOf = gh:
	insteadOf = github:
[url "git@github.com:standard-analytics/"]
	insteadOf = sa:
[url "git@github.com:callahanchris/"]
    insteadOf = cc: 
